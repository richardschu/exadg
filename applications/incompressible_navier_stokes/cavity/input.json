{
    "General": {
        "Precision": "double",
        "Dim": "2",
        "IsTest": "false"
    },
    "SpatialResolution": {
        "DegreeMin": "4",
        "DegreeMax": "4",
        "RefineSpaceMin": "6",
        "RefineSpaceMax": "6"
    },
    "TemporalResolution": {
        "RefineTimeMin": "0",
        "RefineTimeMax": "0"
    },
    "Application": {
        "ProblemType": "Unsteady",
        "IncludeConvectiveTerm": "true",
        "StartTime": "0.0",
        "EndTime": "100.0e-2",
        "CFL": "-1.0",
        "TimeStepSize": "1.0e-2",
        "LidVelocity": "10.0",
        "Density": "1.0",
        "KinematicViscosity": "1e-3",
        "UseGeneralizedNewtonianModel": "true",
        "TemporalDiscretization" : "BDFDualSplittingScheme",
        "TreatmentOfConvectiveTerm" : "Explicit",
        "TreatmentOfVariableViscosity" : "Explicit",
        "GeneralizedNewtonianViscosityMargin": "100e-3",
        "GeneralizedNewtonianKappa": "1.0",
        "GeneralizedNewtonianLambda": "10.0",
        "GeneralizedNewtonianA": "2.0",
        "GeneralizedNewtonianN": "0.5",
        "AbsTolLin": "1.0e-12",
        "RelTolLin": "1.0e-6",
        "AbsTolNewton": "1.0e-12",
        "RelTolNewton": "1.0e-6",
        "AbsTolLinInNewton": "1.0e-12",
        "RelTolLinInNewton": "1.0e-6",
        "IterationsEigenValueEstimationVelocity": "20",
        "IterationsEigenValueEstimationPressureSchur": "20",
        "SchurComplementPreconditioner": "CahouetChabard",
        "IterativeSolveVelocityBlock": "false",
        "IterativeSolvePressureBlock": "false",
        "AbsTolLinBlockInPreconditioner": "1e-12",
        "RelTolLinBlockInPreconditioner": "1e-12"
    },
    "Output": {
        "OutputDirectory": "output/",
        "OutputName": "test",
        "WriteOutput": "true"
    }
}
